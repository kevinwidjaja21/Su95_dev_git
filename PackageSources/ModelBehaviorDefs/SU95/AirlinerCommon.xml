			<Condition Check="SEQ2_EMISSIVE_CODE">
				<True>
					<SEQ2_EMISSIVE_CODE>#SEQ2_EMISSIVE_CODE# (L:XMLVAR_SWITCH_OVHD_INTLT_ANNLT_Position) 0 == or #BLOCK_SEQ2# ! and</SEQ2_EMISSIVE_CODE>
